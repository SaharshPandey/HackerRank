import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner cin=new Scanner(System.in);
		int l=1;
		while(cin.hasNext()){
           
			System.out.println(String.valueOf(l)+" "+cin.nextLine());
             l++;
       
    }
    }}
